{
  "name": "@deployi/workflow",
  "version": "1.0.0",
  "description": "Workflow package for Deployi",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./src/index.ts",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lintfix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/jmespath": "^0.15.0",
    "@types/luxon": "3.2.0",
    "@types/md5": "^2.3.5"
  },
  "dependencies": {
    "@n8n/tournament": "1.0.6",
    "ast-types": "0.15.2",
    "axios": "^1.11.0",
    "callsites": "^4.2.0",
    "deep-equal": "2.2.0",
    "esprima-next": "5.8.4",
    "form-data": "^4.0.4",
    "jmespath": "0.16.0",
    "js-base64": "3.7.2",
    "jssha": "3.3.1",
    "lodash": "4.17.21",
    "luxon": "^3.7.1",
    "md5": "2.3.0",
    "recast": "0.21.5",
    "title-case": "3.0.3",
    "transliteration": "2.3.5"
  }
}
